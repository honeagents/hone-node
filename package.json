{
  "name": "@honeagents/hone",
  "version": "0.1.2",
  "description": "Hone SDK - AI Experience Engineering Platform for tracking and improving LLM applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:unit": "vitest run",
    "typecheck": "tsc --noEmit",
    "ci": "npm run lint && npm run typecheck && npm run test:unit && npm run build",
    "prepublishOnly": "npm run ci"
  },
  "keywords": [
    "ai",
    "llm",
    "tracing",
    "evaluation",
    "observability",
    "langsmith",
    "hone",
    "openai",
    "anthropic",
    "vercel-ai-sdk",
    "ai-sdk"
  ],
  "author": "Hone Team <team@honeagents.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/honeagents/hone-node.git"
  },
  "homepage": "https://honeagents.ai",
  "bugs": {
    "url": "https://github.com/honeagents/hone-node/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "openai": "^6.16.0",
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
